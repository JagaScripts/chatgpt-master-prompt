{
  "name": "chatgpt-master-prompt",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "npm run lint:eslint && npm run lint:html",
    "lint:eslint": "eslint . --ext .ts,.js,.cjs --max-warnings=0",
    "lint:html": "eslint . --ext .html --max-warnings=0",
    "format": "npx prettier --write \"**/*.{ts,js,html,scss,md,json,cjs}\"",
    "test:quick": "echo '(placeholder)'"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-html": "^8.1.3",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-tsdoc": "^0.4.0",
    "prettier": "^3.6.2"
  }
}
